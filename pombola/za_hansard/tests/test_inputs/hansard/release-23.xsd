		    <xsd:complexType mixed="true">
			      <xsd:complexContent>
				        <xsd:extension base="inline">
					          <xsd:attributeGroup ref="link"/>
					          <xsd:attribute name="level" type="xsd:integer" use="required"/>
				        </xsd:extension>
			      </xsd:complexContent>
		    </xsd:complexType>
	  </xsd:element>

	

