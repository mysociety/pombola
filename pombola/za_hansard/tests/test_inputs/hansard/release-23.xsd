				        <xsd:extension base="inlinereqreq">
					          <xsd:attributeGroup ref="role"/>
					          <xsd:attribute name="for" type="xsd:anyURI"/>
					          <xsd:attribute name="empoweredBy" type="xsd:anyURI"/>
