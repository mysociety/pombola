				     <xsd:attributeGroup ref="source"/>
		    </xsd:complexType>
	  </xsd:element>

	  <xsd:element name="note" type="blocksreq">
      <xsd:annotation>
			      <xsd:documentation>
			         <type>Element</type>
            <name>note</name>
            <comment>
The element note is a metadata element containing the text of the footnote and endnote specified.</comment>
				     </xsd:documentation>
		    </xsd:annotation>
   </xsd:element>
		


	  <xsd:element name="proprietary">
		    <xsd:annotation>
			      <xsd:documentation>
			         <type>Element</type>
            <name>proprietary</name>
